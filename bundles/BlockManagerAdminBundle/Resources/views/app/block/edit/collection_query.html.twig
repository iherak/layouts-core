{% if collections.default is defined %}
    {% set collection = collections.default %}

    {% if collection.type is constant('TYPE_DYNAMIC', collection) %}
        <div data-query-form="true" data-form="{{ path('ngbm_app_collection_query_form_edit', {'queryId': collection.query.id, 'formName': 'full'}) }}"></div>
    {% endif %}
{% endif %}
